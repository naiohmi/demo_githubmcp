{
  "servers": {
    "github": {
      "name": "GitHub MCP Server",
      "description": "GitHub integration using MCP server",
      "command": "mcp_server/github-mcp-server",
      "args": ["stdio", "--log-file", "/dev/null"],
      "transport": "stdio",
      "environment_variables": [
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "GITHUB_HOST"
      ],
      "enabled": true,
      "tool_prefix": "github"
    }
  },
  "global_config": {
    "log_level": "ERROR",
    "suppress_mcp_logging": true,
    "tool_timeout": 30
  }
}